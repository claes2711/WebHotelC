@model IEnumerable<WebHotel.Models.Booking>

@{
    ViewData["Title"] = "CustomerIndex";
}

<h2>CustomerIndex</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CheckIn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CheckOut)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TheCustomer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TheRoom)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CheckIn)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CheckOut)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cost)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TheCustomer.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TheRoom.ID)
                </td>
            </tr>
        }
    </tbody>
</table>
